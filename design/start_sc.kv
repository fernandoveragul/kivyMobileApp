<StartScreen>:
  MDRelativeLayout:
    id: stt
    StartScreenLayout:
      id: start_screen_layout
      MDScrollView:
        size_hint_y: .5
        MDLabel:
          id: summary_app
          markup: True
          halign: 'center'
          valign: 'top'
          adaptive_height: True
          text: app.summary_app * 10
      StartScreenBar:
        icon: 'power'
        size_hint_y: .06
        right_action_items: [['menu', lambda x: app.open_close_menu()]]
        # navigation_drawer.set_state('open')
    MDFloatingActionButton:
      id: stt_btn
      icon: 'power'
      elevation: 2.6
      md_bg_color: '#dcff42'
      pos_hint: {'center_x': .5, 'center_y': .053}
      size: '56dp', '56dp'
      on_release: print(self.elevation)